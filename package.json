{
    "name": "sass-compact",
    "displayName": "sass-compact",
    "description": "let's go back to original way to write our css",
    "version": "0.0.5",
    "publisher": "jsonchou",
    "Keywords": "sass compact,less compact,format css,single line,single row",
    "author": {
        "name": "jsonchou",
        "url": "https://github.com/jsonchou"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/jsonchou/sassCompact.git"
    },
    "browserslist": [
        "last 2 versions",
        "Firefox ESR",
        "> 1%",
        "ie >= 9",
        "iOS >= 9",
        "Android >= 4"
    ],
    "bugs": {
        "url": "https://github.com/jsonchou/sassCompact/issues",
        "email": "jsonchou@outlook.com"
    },
    "icon": "name.jpg",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:extension.sassCompact",
        "onLanguage:scss"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "sass compact",
            "properties": {
                "sassCompact.Author": {
                    "type": "string",
                    "default": "jsonchou",
                    "description": "author description"
                },
                "sassCompact.Less": {
                    "type": "boolean",
                    "default": false,
                    "description": "enable less"
                }
            }
        },
        "keybindings": [
            {
                "command": "extension.sassCompact",
                "key": "shift+alt+f",
                "when": "editorTextFocus && editorLangId == scss"
            },
            {
                "command": "extension.sassCompact",
                "key": "shift+alt+f",
                "when": "editorTextFocus && editorLangId == less"
            },
            {
                "command": "extension.sassCompact",
                "key": "shift+alt+f",
                "when": "editorTextFocus && editorLangId == wxss"
            }
        ],
        "commands": [
            {
                "command": "extension.sassCompact",
                "title": "sassCompact"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "cd test && node index.js",
        "server": "NODE_ENV=production  GATEWAY=outer    node ./server/index.js",
        "clean": "node ./build/scripts/clean",
        "release": "node ./build/scripts/release"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "autoprefix": "^1.0.1",
        "done-rainbow": "^0.1.2",
        "fs-extra": "^8.1.0",
        "glob": "^7.1.4",
        "postcss": "^7.0.17",
        "postcss-advanced-variables": "^3.0.0",
        "postcss-define-function": "^0.1.2",
        "postcss-extend": "^1.0.5",
        "postcss-mixins": "^6.2.2",
        "postcss-nested": "^4.1.2",
        "postcss-nested-import": "^0.1.0",
        "postcss-nested-vars": "^1.0.0",
        "postcss-sassy-mixins": "^2.1.0",
        "postcss-scss": "^2.0.0",
        "postcss-simple-extend": "^1.0.0",
        "postcss-simple-vars": "^5.0.2",
        "precss": "^4.0.0",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    }
}
